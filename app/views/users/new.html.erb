<h1>Register a new user</h1>
<div id='new_user_form'>
    <%= form_with url: users_path, local: true do |f| %>
        <%= f.label :name, "Name:" %>
        <%= f.text_field :name %>
        <%= f.label :email, "Email:" %>
        <%= f.text_field :email %>
        <%= f.submit 'Register' %>
    <% end %>
